	<title>Matches</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="/css/introHCI.css" rel="stylesheet">
	<link href="font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet">


</head>

<body>

	<h2 style="text-align:center">Matches</h1>

	<h4 style= "text-align:center">Searching for: {{prefs.gender}} {{prefs.mbti}}, Ages {{prefs.minage}}-{{prefs.maxage}}, in {{prefs.location}}</h3>


		<div id="matches" class="container" style="margin-bottom:20vh">
			<div class="row">
			{{#each users}}
				{{#isNth @index 2}}
					</div>
					<div class="row">
				{{/isNth}}
				<div class="col-xs-6">
					<div class="match" style="width:auto;margin:2%;padding:5%">
						<div class="matchName" style="display:-webkit-box">
							<h4 style="text-align:left;width:70%;height:20px;overflow:hidden">{{name}}</h4>
							<h4 style="text-align:right;width:30%">{{age}}</h4>
						</div>
						<div style="display:block">
							<div>
								<img src={{image}} width=100% height=100% style="border-radius:5%">
							</div>
							<div>
								<button class="btn btn-primary btn-lg btn-block" onclick="chat('{{username}}')">
                                <script>
                                    function chat(username){
																				ga('send', 'event', 'Matches', "Chatclick");
                                        window.location="/chat/" + username;
                                    }
                                </script>
                                {{../buttonText}}</button>
							</div>
						</div>
					</div>
				</div>
			{{/each}}
			</div>
		</div>




		<div id="navbar" class="navigation">
			<ul>
				<li id="settingsbutton">
					<span class="fa fa-cogs" aria-hidden="true"></span>
					<p>SETTINGS</p>
				</li>
				<div id=nonsettings>
					<li id="matcheslink">
						<span class="fa fa-heart-o" aria-hidden="true"></span>
						<p>MATCHES</p>
					</li>
					<li id="mbtilink">
						<span class="fa fa-bar-chart" aria-hidden="true"></span>
						<p>MBTI</p>
					</li>
					<li id="chatlink">
						<span class="fa fa-inbox" aria-hidden="true"></span>
						<p>CHAT</p>
					</li>
				</div>
				<div id=settings style="display:none">
					<li id="editlink">
						<span class="fa fa-pencil-square-o " aria-hidden="true"></span>
						<p>EDIT PROFILE</p>
					</li>
					<li id="preflink">
						<span class="fa fa-search" aria-hidden="true"></span>
						<p>PREFERENCES</p>
					</li>
					<li id="logoutlink">
						<span class="fa fa-sign-out" aria-hidden="true"></span>
						<p>LOGOUT</p>
					</li>
				</div>
			</ul>
		</div>
	</div>

	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="/js/bootstrap.min.js"></script>
	<script src="/js/navbar.js"></script>
	<script src="/js/user.js"></script>
