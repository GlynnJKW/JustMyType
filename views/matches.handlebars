<!doctype html>
<html>
<head>
	<title>Matches</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="/css/introHCI.css" rel="stylesheet">
	<link href="font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet">


</head>

<body>

	<h1 style="text-align:center">Matches</h1>

		<div id="matches" class="matches">
			{{#each users}}
				<div class="match">
					<div class="matchName">
						<h1>{{name}}</h1>
					</div>
					<div class="matchInfo">
						<div class="matchPhoto">
							<img src={{image}} width=100% height=100% style="border-radius:5%">
						</div>
						<div class="matchDesc">
							<h3>{{age}} year old {{gender}}</h3>
							<h2>{{mbti}}</h2>
						</div>
					</div>
				</div>
			{{/each}}
		</div>




		<div id="navbar" class="navigation">
			<ul>
				<li id="matcheslink">
					<span class="fa fa-heart-o" aria-hidden="true"></span>
					<p>MATCHES</p>
				</li>
				<li id="mbtilink">
					<span class="fa fa-bar-chart" aria-hidden="true"></span>
					<p>MBTI</p>
				</li>
				<li id="chatlink">
					<span class="fa fa-inbox" aria-hidden="true"></span>
					<p>CHAT</p>
				</li>
				<li id="profilelink">
					<span class="fa fa-user-circle" aria-hidden="true"></span>
					<p>PROFILE</p>
				</li>
			</ul>
		</div>
	</div>



		<script src="https://code.jquery.com/jquery.js"></script>
	<script src="/js/bootstrap.min.js"></script>
	<script src="/js/navbar.js"></script>

</body>
</html>
